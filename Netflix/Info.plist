<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Google Sign-In için URL Scheme -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <!-- GoogleService-Info.plist içindeki REVERSED_CLIENT_ID -->
                <string>com.googleusercontent.apps.872077491698-td0ump0773ivlccp8otsst95plc0as7d</string>
            </array>
        </dict>
    </array>

    <!-- Google Sign-In için gerekli izinler -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
        <string>com.googleusercontent.apps.872077491698-td0ump0773ivlccp8otsst95plc0as7d</string>
        <string>google</string>
        <string>com.google</string>
    </array>

    <!-- SceneDelegate ayarları -->
    <key>UIApplicationSceneManifest</key>
    <dict>
        <key>UIApplicationSupportsMultipleScenes</key>
        <false/>
        <key>UISceneConfigurations</key>
        <dict>
            <key>UIWindowSceneSessionRoleApplication</key>
            <array>
                <dict>
                    <key>UISceneConfigurationName</key>
                    <string>Default Configuration</string>
                    <key>UISceneDelegateClassName</key>
                    <string>$(PRODUCT_MODULE_NAME).SceneDelegate</string>
                    <key>UISceneStoryboardFile</key>
                    <string>Main</string>
                </dict>
            </array>
        </dict>
    </dict>
</dict>
</plist>
